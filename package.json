{
  "name": "kladovka",
  "version": "1.0.0",
  "description": "Optimizer of inventory for games",
  "main": "main.js",
  "scripts": {
    "lint": "eslint . --format unix",
    "deps": "yarn install && cd frontend && yarn install --modules-folder ../node_modules && cd ../backend && yarn install --modules-folder ../node_modules && cd ../domain && yarn install --modules-folder ../node_modules && cd ../e2e && yarn install --modules-folder ../node_modules",
    "start": "parallelshell 'npm run start:backend' 'npm run start:frontend'",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "test": "cd domain && npm test && cd ../e2e && npm start",
    "clean": "cd frontend && npm clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emotz/kladovka.git"
  },
  "keywords": [
    "kladovka",
    "games",
    "emotz"
  ],
  "author": "emotz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emotz/kladovka/issues"
  },
  "homepage": "https://github.com/emotz/kladovka#readme",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-webdriverio": "^1.0.1"
  },
  "dependencies": {
    "parallelshell": "^3.0.1"
  }
}
